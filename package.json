{
  "name": "@zcong/auto-sequelize-typescript",
  "version": "0.4.0",
  "description": "auto generate model files from exists db schemas",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "bin": {
    "auto-seq-ts": "./dist/bin/index.js"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "test": "npm run lint && npm run jest",
    "test:cov": "npm run lint && npm run jest:cov",
    "lint": "tslint -p .",
    "format": "prettier --write '**/*.{ts,json,md}'",
    "jest": "jest --detectOpenHandles --forceExit",
    "jest:cov": "jest --coverage --detectOpenHandles --forceExit",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf models dist",
    "prepare": "husky install"
  },
  "types": "./dist/index.d.ts",
  "author": "zcong1993",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.4.13",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "sequelize-typescript": "^2.1.0",
    "ts-jest": "^27.0.4",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typeorm": "^0.2.36",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "lint-staged": {
    "*.{js,json,ts}": "prettier --write"
  },
  "dependencies": {
    "cac": "^6.7.3",
    "camelcase": "^6.2.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "mysql2": "^2.3.0",
    "sequelize": "^6.6.5"
  }
}

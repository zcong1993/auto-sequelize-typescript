{
  "name": "@zcong/auto-sequelize-typescript",
  "version": "0.5.1",
  "description": "auto generate model files from exists db schemas",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "auto-seq-ts": "./dist/bin/index.js"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "test": "npm run lint && npm run jest",
    "test:cov": "npm run lint && npm run jest:cov",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write '**/*.{ts,json,md}'",
    "jest": "jest --detectOpenHandles --forceExit",
    "jest:cov": "jest --coverage --detectOpenHandles --forceExit",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf models dist",
    "prepare": "husky install",
    "release": "js-release"
  },
  "types": "./dist/index.d.ts",
  "author": "zcong1993",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@zcong/js-release": "^0.2.5",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.2.2",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "sequelize-typescript": "^2.1.1",
    "ts-jest": "^27.0.7",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typeorm": "^0.2.38",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "lint-staged": {
    "*.{js,json,ts,md}": "prettier --write"
  },
  "dependencies": {
    "cac": "^6.7.11",
    "camelcase": "^6.2.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "mysql2": "^2.3.2",
    "sequelize": "^6.9.0"
  }
}
